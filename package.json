{
  "name": "@paybuy/cints-core",
  "version": "0.0.141",
  "main": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "npm run clean & concurrently \"npm run build:watch\" \"npm run start:watch\"",
    "build": "tsc --project tsconfig.json && swc src -d dist && rollup -c rollup.config.js",
    "build:watch": "swc -w src -d dist",
    "start": "node dist/index.js",
    "start:watch": "nodemon --watch \"dist/**/*\" -e js ./dist/index.js",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.96",
    "@types/node": "^20.14.10",
    "concurrently": "^8.2.2",
    "gts": "^5.2.0",
    "nodemon": "^3.0.1",
    "rollup": "^4.18.0",
    "rollup-plugin-dts": "^6.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@types/lodash": "^4.14.201",
    "axios": "^1.6.0",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.5",
    "yup": "^1.3.3"
  }
}
